    "@types/rimraf": "^2.0.3",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ethers": "^4.0.39",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "ts-generator": "0.0.8",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
